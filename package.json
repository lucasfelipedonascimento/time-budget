{
  "name": "timebudget",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --env-file .env --experimental-strip-types --no-warnings src/server.ts",
    "dev": "node --env-file .env --experimental-strip-types --no-warnings --watch src/server.ts",
    "db:seed": "node --env-file .env --experimental-strip-types --no-warnings --watch src/db/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --setup ./vitest.setup.unit.ts --testNamePattern unit",
    "test:integration": "vitest run  --setup ./vitest.setup.integration.ts --testNamePattern integration"
  },
  "engines": {
    "node": "22"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/express": "^5.0.3",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@faker-js/faker": "^9.9.0",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.4",
    "@vitest/coverage-v8": "^3.2.4",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "typescript": "^5.8.3",
    "ultracite": "^5.1.1",
    "vitest": "^3.2.4"
  }
}
